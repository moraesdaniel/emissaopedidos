program EmissaoPedidos;

uses
  Vcl.Forms,
  uFrmPrincipal in 'view\uFrmPrincipal.pas' {frmPrincipal},
  uFrmCadastroItem in 'view\uFrmCadastroItem.pas' {frmCadastroItem},
  uItemModel in 'model\uItemModel.pas',
  uItemControler in 'control\uItemControler.pas',
  uDmConexao in 'dao\uDmConexao.pas' {DmConexao: TDataModule},
  uDmItem in 'dao\uDmItem.pas' {DmItem: TDataModule};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TfrmPrincipal, frmPrincipal);
  Application.CreateForm(TfrmCadastroItem, frmCadastroItem);
  Application.CreateForm(TDmConexao, DmConexao);
  Application.CreateForm(TDmItem, DmItem);
  Application.Run;
end.
